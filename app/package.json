{
  "name": "vrpair",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "test:e2e": "detox test -c ios.sim.debug",
    "build:e2e": "detox build -c ios.sim.debug",
    "ci:test:e2e": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:e2e": "detox build -c ios.sim.release",
    "tsc": "tsc -p .",
    "lint": "clear && eslint index.js app --fix --ext .js,.jsx,.ts,.tsx",
    "patch": "patch-package",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "test": "jest",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "barrels": "barrelsby -DS -d ./app/components -l replace -e _ -e index",
    "openapi": "cd ../backend && make schema",
    "orval": " rm -rf app/api/* && (cd ../../libraries/orval && npm run dev) && eslint index.js app/api --fix --ext .ts",
    "apiClient": "npm run openapi && npm run orval"
  },
  "dependencies": {
    "@miblanchard/react-native-slider": "~2.1.0",
    "@react-native-community/masked-view": "~0.1.11",
    "@react-native-picker/picker": "~2.3.0",
    "@react-navigation/bottom-tabs": "~6.2.0",
    "@react-navigation/native": "~6.0.8",
    "@react-navigation/native-stack": "~6.5.0",
    "@react-navigation/stack": "~6.1.1",
    "@unimodules/core": "~7.1.2",
    "date-fns": "^2.28.0",
    "ky": "^0.29.0",
    "mobx": "^6.4.2",
    "mobx-react-lite": "^3.3.0",
    "mobx-state-tree": "^5.1.3",
    "native-base": "~3.3.7",
    "react": "~17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.27.1",
    "react-native": "~0.67.3",
    "react-native-app-auth": "~6.4.3",
    "react-native-gesture-handler": "~2.3.0",
    "react-native-keychain": "~8.0.0",
    "react-native-mmkv": "~2.1.0",
    "react-native-pager-view": "~5.4.11",
    "react-native-reanimated": "~2.4.1",
    "react-native-reanimated-carousel": "~2.3.0",
    "react-native-safe-area-context": "~4.0.1",
    "react-native-screens": "~3.12.0",
    "react-native-svg": "~12.1.1",
    "react-native-tab-view": "~3.1.1",
    "react-native-ui-lib": "~6.10.1",
    "react-native-unimodules": "~0.14.10",
    "react-query": "^3.34.16",
    "styled-system": "^5.1.5",
    "use-async-effect": "^2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-proposal-optional-catch-binding": "^7.16.7",
    "@babel/runtime": "^7.17.2",
    "@react-navigation/devtools": "^6.0.5",
    "@storybook/addon-storyshots": "^6.4.19",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@types/react-native": "^0.67.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@welldone-software/why-did-you-render": "^6.2.3",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "barrelsby": "^2.3.3",
    "detox": "^19.4.5",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fbjs-scripts": "^3.0.1",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "jest-expo": "^44.0.1",
    "jetifier": "^2.0.0",
    "mobx-flipper": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "orval": "^6.6.4",
    "patch-package": "^6.4.7",
    "postinstall-prepare": "^2.0.0",
    "prettier": "^2.5.1",
    "react-devtools-core": "^4.23.0",
    "react-dom": "^17.0.2",
    "react-native-clean-project": "^4.0.1",
    "react-native-flipper": "^0.136.0",
    "react-native-web": "^0.17.6",
    "react-powerplug": "^1.0.0",
    "solidarity": "^3.0.4",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-native-async-storage|@react-navigation|@storybook|@react-native-community|@unimodules)"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/vrpair.app",
        "build": "xcodebuild -workspace ios/vrpair.xcworkspace -scheme vrpair -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 11'",
        "type": "ios.simulator",
        "name": "iPhone 11"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/vrpair.app",
        "build": "xcodebuild -workspace ios/vrpair.xcworkspace -scheme vrpair -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 11'",
        "type": "ios.simulator",
        "name": "iPhone 11"
      }
    }
  }
}
